    rdfs:domain tdoc:SessionNotes ;
    rdfs:range tdoc:Company .

# -----------------------------------------------------------------------------
# Company-specific Object Property (1)
# -----------------------------------------------------------------------------

tdoc:rapporteurOf a owl:ObjectProperty ;
    rdfs:label "rapporteur of"@en ;
