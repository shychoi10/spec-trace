tdoc:resolutionBelongsTo a owl:ObjectProperty ;
    rdfs:label "resolution belongs to"@en ;
    rdfs:comment "Agenda Item under which this Resolution was made"@en ;
    rdfs:domain tdoc:Resolution ;
    rdfs:range tdoc:AgendaItem .

tdoc:madeAt a owl:ObjectProperty ;
    rdfs:label "made at"@en ;
    rdfs:comment "Meeting where this Resolution was made"@en ;
    rdfs:domain tdoc:Resolution ;
    rdfs:range tdoc:Meeting .

tdoc:hasResolutions a owl:ObjectProperty ;
    rdfs:label "has resolutions"@en ;
    rdfs:comment "Resolutions made under this AgendaItem"@en ;
    rdfs:domain tdoc:AgendaItem ;
    rdfs:range tdoc:Resolution ;
    owl:inverseOf tdoc:resolutionBelongsTo .

tdoc:promotedTo a owl:ObjectProperty ;
    rdfs:label "promoted to"@en ;
    rdfs:comment "Agreement that this WorkingAssumption was promoted to"@en ;
    rdfs:domain tdoc:WorkingAssumption ;
    rdfs:range tdoc:Agreement .

